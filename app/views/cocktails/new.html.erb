<h1>New Cocktail</h1>
<%= simple_form_for(@cocktail) do |f| %>
  <%= f.input :name %>
  <%= f.input :instruction %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit "Save", class: "btn btn-danger" %>
<% end %>

<%= link_to cocktails_path, class: "btn btn-primary" do %>
    Back
<% end %>

